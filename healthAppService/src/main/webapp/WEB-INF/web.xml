<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_9" version="3.0" xmlns="http://java.sun.com/xml/ns/j2ee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaeehttp://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
	<display-name>healthAppService</display-name>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	<listener>
		<listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
	</listener>
	<context-param>
		<param-name>contextConfigLocation</param-name>
		<param-value>classpath:applicationContext.xml</param-value>
	</context-param>

	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.health.servlets.DelegatingServletProxy</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Register</servlet-name>
		<servlet-class>com.health.servlets.DelegatingServletProxy</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>GetUserDetails</servlet-name>
		<servlet-class>com.health.servlets.DelegatingServletProxy</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UploadUserPhoto</servlet-name>
		<servlet-class>com.health.servlets.DelegatingServletProxy</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>PutUserPhoto</servlet-name>
		<servlet-class>com.health.servlets.DelegatingServletProxy</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>GetUserDynamic</servlet-name>
		<servlet-class>com.health.servlets.DelegatingServletProxy</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>PutUserDetails</servlet-name>
		<servlet-class>com.health.servlets.DelegatingServletProxy</servlet-class>
	</servlet>
	<servlet>
		
		<servlet-name>GetDetails</servlet-name>
		<servlet-class>com.health.servlets.DelegatingServletProxy</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>PutHistory</servlet-name>
		<servlet-class>com.health.servlets.DelegatingServletProxy</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>PutUserDynamic</servlet-name>
		<servlet-class>com.health.servlets.DelegatingServletProxy</servlet-class>
	</servlet>


	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Register</servlet-name>
		<url-pattern>/Register</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>GetUserDetails</servlet-name>
		<url-pattern>/GetUserDetails</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UploadUserPhoto</servlet-name>
		<url-pattern>/UploadUserPhoto</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PutUserPhoto</servlet-name>
		<url-pattern>/PutUserPhoto</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>GetUserDynamic</servlet-name>
		<url-pattern>/GetUserDynamic</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PutUserDetails</servlet-name>
		<url-pattern>/PutUserDetails</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>GetDetails</servlet-name>
		<url-pattern>/GetDetails</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PutHistory</servlet-name>
		<url-pattern>/PutHistory</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PutUserDynamic</servlet-name>
		<url-pattern>/PutUserDynamic</url-pattern>
	</servlet-mapping>

</web-app>